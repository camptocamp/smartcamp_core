<odoo>
    <record model="ir.ui.view" id="mis_total_committed_purchase_tree_view">
        <field name="name">mis.total.committed.purchase.tree</field>
        <field name="model">mis.total.committed.purchase</field>
        <field name="arch" type="xml">
            <tree>
                <field name="date" />
                <field name="name" />
                <field name="purchase_order_id" />
                <field name="account_id" />
                <field
                    name="analytic_account_id"
                    groups="analytic.group_analytic_accounting"
                />
                <field
                    name="analytic_tag_ids"
                    widget="many2many_tags"
                    groups="analytic.group_analytic_tags"
                />
                <field name="product_id" />
                <field name="debit" />
                <field name="credit" />
            </tree>
        </field>
    </record>
</odoo>
